                <FunctionBlock UUID="F6D81CF545166B8DEA829DAAA4FFC5AD" Name="drum_NO">
                    <Variables />
                    <ST><![CDATA[drumNO.start := (APS_TASK = INT#4) AND MD.NO;

// Пуск drum
IF drumNO.start AND NOT drumNO.run THEN
	drumNO.run := BOOL#1;
	drumNO.step := INT#0;
END_IF;

// Останов drum извне
IF MD.EAO_KS OR MD.EAO_KC2 OR MD.AOS OR MD.AOB OR FirstScan_MAIN THEN
	drumNO.run := BOOL#0;
	drumNO.step_prev := INT#-1;
	drumNO.step := INT#-1;
	NO_to_DO := BOOL#0;
//	NO_KR36_ON := BOOL#0;
//	NO_KR36a_ON := BOOL#0;
//	NO_KR23_OFF := BOOL#0;
//	NO_KR25_OFF := BOOL#0;
	//NO_start := BOOL#0;
	//NO_stop := BOOL#0;
END_IF;

// Тело drum
CASE drumNO.step OF
0:	IF drumNO.step_prev <> INT#0 THEN
		drumNO.TMR2 := LREAL#900;
		drumNO.step_prev := drumNO.step;
		//NO_start := BOOL#1;
		CREATE_EVENT('Запуск алгоритма НО КЦ-2', DINT#102, INT#1, 'КЦ', '', CURRENT_DATE_TIME(), '');
	END_IF;
	NO_to_DO := BOOL#1; (*НО в ГПА*)
	(*КХР открыть*)
//	NO_KR36_ON := BOOL#1;
//	NO_KR36a_ON := BOOL#1;
//	NO_KR23_OFF := BOOL#1;
//	NO_KR25_OFF := BOOL#1;
	drumNO.TMR2 := drumNO.TMR2 - cycleTime_MAIN;

	(*IF (CTU1_SHKU17_A1.KR7_OFF AND NOT CTU1_SHKU17_A1.KR7_ON) AND (CTU1_SHKU17_A1.KR7a_OFF AND NOT CTU1_SHKU17_A1.KR7a_ON) AND (CTU1_SHKU17_A1.KR8_OFF AND NOT CTU1_SHKU17_A1.KR8_ON) AND
	   (SHKU17_DI_HMI_A1.KR36_ON AND NOT SHKU17_DI_HMI_A1.KR36_OFF) AND (SHKU17_DI_HMI_A1.KR36a_ON AND NOT SHKU17_DI_HMI_A1.KR36a_OFF) AND
	   (SHKU17_DI_HMI_A1.KR36p_ON AND NOT SHKU17_DI_HMI_A1.KR36p_OFF) AND
	   (SHKU17_DI_HMI_A1.KR23_OFF AND NOT SHKU17_DI_HMI_A1.KR23_ON) AND (SHKU17_DI_HMI_A1.KR25_OFF AND NOT SHKU17_DI_HMI_A1.KR25_ON) AND
	   (MD_Teh.GPAs_stop) THEN
		drumNO.step := INT#1;
	END_IF; *)
	IF drumNO.TMR2 <= LREAL#0 THEN
		drumNO.step := INT#2;
	END_IF;
1:	drumNO.done_ok := BOOL#1;
	CREATE_EVENT('Нормальное завершение алгоритма НО КЦ-2', DINT#102, INT#1, 'КЦ', '', CURRENT_DATE_TIME(), '');
	//NO_stop := BOOL#1;
	drumNO.step_prev := drumNO.step;
	drumNO.step := INT#3;
2:	drumNO.done_flt := BOOL#1;
	drumNO.step_prev := drumNO.step;
	drumNO.step := INT#3;
3:	IF drumNO.step_prev <> INT#3 THEN
		NO_to_DO := BOOL#0;
		//NO_start := BOOL#0;
//		NO_KR36_ON := BOOL#0;
//		NO_KR36a_ON := BOOL#0;
//		NO_KR23_OFF := BOOL#0;
//		NO_KR25_OFF := BOOL#0;
		drumNO.step_prev := drumNO.step;
	END_IF;
	IF Deblock AND NOT drumNO.start THEN
		//NO_stop := BOOL#0;
		drumNO.step := INT#-1;
