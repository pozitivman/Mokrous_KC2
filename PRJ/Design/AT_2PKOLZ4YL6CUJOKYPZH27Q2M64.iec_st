                    <Variable UUID="438DA8FF4E97D64D5B88A3B22BA5BAFC" Name="temp_mode_string" Type="STRING" TypeUUID="38FDDE3B442D86554C56C884065F87B7" InitialValue="&apos;FALSE&apos;" Usage="internal" />
                    <Variable UUID="B93B51FB4209E9E10585049214AB37EC" Name="temp_status_string" Type="STRING" TypeUUID="38FDDE3B442D86554C56C884065F87B7" Usage="internal" />
                    <Variable UUID="834953464F37A901EC89529DD3BC6735" Name="delay" Type="LREAL" TypeUUID="65F1DDD44EDA9C0776BB16BBDFE36B1F" Usage="internal" />
                    <Variable UUID="2AB1FCD74C1D19724DAB909367C15212" Name="ESDb" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
                    <Variable UUID="1402FB7E4C2187BC8303859A70CFF006" Name="ESDw" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
                    <Variable UUID="77138F2740085DFD13B28CA591DFA325" Name="NSDb" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
                    <Variable UUID="AFEEA4FB448312A58B1990B336F4EB30" Name="NSDw" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
                    <Variable UUID="E2C141014F9720A7CEF1C79B9D10A316" Name="KOLg" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
                </Variables>
                <ST><![CDATA[ESDb := APS_TASK = INT#6; // Аварийный останов со стравливанием
ESDw := APS_TASK = INT#5; // Аварийный останов без стравливания
NSDb := APS_TASK = INT#4; // Нормальный останов
NSDw := APS_TASK = INT#3; // КОЛЬЦО КЦ-1
KOLg := APS_TASK = INT#2; // Кольцо ГПА КЦ-1

//////////////////////////////////////////////////////////////////////////////////////////
temp_mode_string := '';
temp_status_string := '';
// Экстренный останов КС
//IF  THEN
//	MD.EAO_KS  := BOOL#1;
//	delay := LREAL#300;
//END_IF;

// Экстренный останов КЦ-2
IF (SHKU17_A3_DI_HMI.EAO_KC_PU OR SHKU17_A3_DI_HMI.EAO_KC_PEAO) AND NOT MD.EAO_KS THEN
	MD.EAO_KC2  := BOOL#1; 
	delay := LREAL#300;
END_IF;
IF Deblock AND (delay <= LREAL#0) THEN
	MD.EAO_KS := BOOL#0;
	MD.EAO_KC2 := BOOL#0;
END_IF;

IF MD.EAO_KC2 THEN
	temp_mode_string := 'ЭО КЦ-2';
	IF delay > LREAL#0.5 THEN
		delay := delay - cycleTime_MAIN;
		temp_status_string := FORMAT_LREAL(delay, UINT#0) + ' сек.';
	ELSE
		delay := LREAL#0;
		temp_status_string := 'Нажмите Сброс';
	END_IF;
END_IF;
IF MD.EAO_KS THEN
	temp_mode_string := 'ЭО КС';
	IF delay > LREAL#0.5 THEN
		delay := delay - cycleTime_MAIN;
		temp_status_string := FORMAT_LREAL(delay, UINT#0) + ' сек.';
	ELSE
		delay := LREAL#0;
		temp_status_string := 'Нажмите Сброс';
	END_IF;
END_IF;

// Аварийный останов со стравливанием
MD.AOS := NOT (MD.EAO_KC2 OR MD.EAO_KS) AND (ESDb OR drumAOS.run);
IF MD.AOS THEN
	temp_mode_string := 'АО со стравливанием';
	IF drumAOS.step = INT#0 THEN
		temp_status_string := 'До завершения: ' + FORMAT_LREAL(drumAOS.TMR2, UINT#0) + ' сек.';
	END_IF;
	IF drumAOS.done_ok THEN
		temp_status_string := 'Алгоритм АО КЦ-2 СС завершён$n$nНажмите Сброс';
	END_IF;
	IF drumAOS.done_flt THEN
		temp_status_string := 'Алгоритм АО КЦ-2 СС завершён некорректно$n$nНажмите Сброс';
	END_IF;
END_IF;

// Аварийный останов без стравливания
MD.AOB := NOT (MD.EAO_KC2 OR MD.EAO_KS OR MD.AOS) AND (ESDw OR drumAOB.run);
IF MD.AOB THEN
	temp_mode_string := 'АО без стравливания';
	IF drumAOB.step = INT#0 THEN
		temp_status_string := 'До завершения: ' + FORMAT_LREAL(drumAOB.TMR2, UINT#0) + ' сек.';
	END_IF;
	IF drumAOB.done_ok THEN
		temp_status_string := 'Алгоритм АО КЦ-2 БС завершён$n$nНажмите Сброс';
	END_IF;
	IF drumAOB.done_flt THEN
		temp_status_string := 'Алгоритм АО КЦ-2 БС завершён некорректно$n$nНажмите Сброс';
	END_IF;
END_IF;

// Нормальный останов
MD.NO := NOT (MD.EAO_KC2 OR MD.EAO_KS OR MD.AOS OR MD.AOB) AND (NSDb OR drumNO.run);
IF MD.NO THEN
	temp_mode_string := 'Нормальный останов';
	IF drumNO.step = INT#0 THEN
		temp_status_string := 'До завершения: ' + FORMAT_LREAL(drumNO.TMR2, UINT#0) + ' сек.';
	END_IF;
	IF drumNO.done_ok THEN
		temp_status_string := 'Алгоритм НО КЦ-2 завершён$n$nНажмите Сброс';
	END_IF;
	IF drumNO.done_flt THEN
		temp_status_string := 'Алгоритм НО КЦ-2 завершён некорректно$n$nНажмите Сброс';
	END_IF;
END_IF;

// КЦ-2 Кольцо
MD.KOL_KC2 := NOT (MD.EAO_KC2 OR MD.EAO_KS OR MD.AOS OR MD.AOB OR MD.NO) AND (NSDw OR drumKOL.run);
IF MD.KOL_KC2 THEN
	temp_mode_string := 'КЦ-2 Кольцо';
	IF drumKOL.step = INT#0 THEN
		temp_status_string := 'До завершения: ' + FORMAT_LREAL(drumKOL.TMR2, UINT#0) + ' сек.';
	END_IF;
	IF drumKOL.done_ok THEN
		temp_status_string := 'Алгоритм КЦ-2 Кольцо завершён$n$nНажмите Сброс';
	END_IF;
	IF drumKOL.done_flt THEN
		temp_status_string := 'Алгоритм КЦ-2 Кольцо завершён некорректно$n$nНажмите Сброс';
	END_IF;
END_IF;

// ГПА Кольцо
MD.KOL_GPA := NOT (MD.EAO_KC2 OR MD.EAO_KS OR MD.AOS OR MD.AOB OR MD.NO OR MD.KOL_KC2) AND (KOLg OR drumKOL_GPA.run);
IF MD.KOL_GPA THEN
	temp_mode_string := 'ГПА Кольцо';
	IF drumKOL_GPA.step = INT#0 THEN
		temp_status_string := 'До завершения: ' + FORMAT_LREAL(drumKOL_GPA.TMR2, UINT#0) + ' сек.';
	END_IF;
	IF drumKOL_GPA.done_ok THEN
		temp_status_string := 'Алгоритм Кольцо ГПА КЦ-2 завершён$n$nНажмите Сброс';
	END_IF;
	IF drumKOL_GPA.done_flt THEN
		temp_status_string := 'Алгоритм Кольцо ГПА КЦ-2 завершён некорректно$n$nНажмите Сброс';
	END_IF;
END_IF;

// КЦ-2 Магистраль
MD.MAG := NOT (MD.EAO_KC2 OR MD.EAO_KS OR MD.AOS OR MD.AOB OR MD.NO OR MD.KOL_KC2 OR MD.KOL_GPA) AND
		  (Link_GPA7.MD_M OR Link_GPA8.MD_M OR Link_GPA9.MD_M) AND true
			(*состояние каких-то кранов*);
IF MD.MAG THEN
	temp_mode_string := 'КЦ-2 Магистраль';
END_IF;

// Пуск КЦ-2
MD.PUSK := NOT (MD.EAO_KC2 OR MD.EAO_KS OR MD.AOS OR MD.AOB OR MD.NO OR MD.KOL_KC2 OR MD.KOL_GPA OR MD.MAG) AND
		  (Link_GPA7.MD_APK OR Link_GPA7.MD_APM OR Link_GPA8.MD_APK OR Link_GPA8.MD_APM OR Link_GPA9.MD_APK OR Link_GPA9.MD_APM);
IF MD.PUSK THEN
	temp_mode_string := 'Пуск КЦ-2';
END_IF;

// КЦ-2 остановлен (контур заполнен газом)
MD.STOP_G := NOT (MD.EAO_KC2 OR MD.EAO_KS OR MD.AOS OR MD.AOB OR MD.NO OR MD.KOL_KC2 OR MD.KOL_GPA OR MD.MAG OR MD.PUSK) AND true
			(*еще какие-то условия*);
IF MD.STOP_G THEN
	temp_mode_string := 'КЦ-2 остановлен (контур заполнен газом)';
END_IF;

// КЦ2 остановлен (контур без газа)
MD.STOP := NOT (MD.EAO_KC2 OR MD.EAO_KS OR MD.AOS OR MD.AOB OR MD.NO OR MD.KOL_KC2 OR MD.KOL_GPA OR MD.MAG OR MD.PUSK OR MD.STOP_G) AND true
			(*еще какие-то условия*);
IF MD.STOP THEN
	temp_mode_string := 'КЦ-2 остановлен (контур без газа)';
END_IF;

// Неопределённый режим
MD.UND :=  NOT (MD.EAO_KC2 OR MD.EAO_KS OR MD.AOS OR MD.AOB OR MD.NO OR MD.KOL_KC2 OR MD.KOL_GPA OR MD.MAG OR MD.PUSK OR MD.STOP_G OR MD.STOP);
IF MD.UND THEN
	temp_mode_string := 'Неопределённый режим';
END_IF;


//Раскраска телевизора
(*жёлтый*)			Global_TV.Color.0 := MD.KOL_KC2 OR MD.KOL_GPA OR MD.NO;
(*красный*)			Global_TV.Color.1 := MD.EAO_KC2 OR MD.EAO_KS OR MD.AOS OR MD.AOB;
(*зелёный*)			Global_TV.Color.2 := MD.PUSK;
(*тёмно-зелёный*)	(*Global_TV.Color.3 := MD.MAG;*)
(*коричневый*)		(*Global_TV.Color.4 := MD.STOP_G OR MD.STOP;*)
(*серый*)			Global_TV.Color.5 := MD.UND OR MD.STOP_G OR MD.STOP;
(*голубой*)			Global_TV.Color.6 := MD.MAG;

///////////////////////////////////////////////////////////////////////////////////////////

Global_TV.LG_System_mode_str := temp_mode_string;
Global_TV.LG_System_status_str := temp_status_string;]]></ST>
            </FunctionBlock>
        </Folder>
