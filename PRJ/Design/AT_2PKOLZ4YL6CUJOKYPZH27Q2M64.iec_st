                <FunctionBlock UUID="5C3F45D04F7362D06D4D46AAFA5B9054" Name="drum_AOB">
                    <Variables />
                    <ST><![CDATA[drumAOB.start := (APS_TASK = INT#5) AND MD.AOB;

// Пуск drum
IF drumAOB.start AND NOT drumAOB.run THEN
	drumAOB.run := BOOL#1;
	drumAOB.step := INT#0;
END_IF;

// Останов drum извне
IF MD.EAO_KS OR MD.EAO_KC2 OR MD.AOS OR FirstScan_MAIN THEN
	drumAOB.run := BOOL#0;
	drumAOB.step_prev := INT#-1;
	drumAOB.step := INT#-1;
	AOB_to_DO := BOOL#0;
//	AOB_KR36_ON := BOOL#0;
//	AOB_KR36a_ON := BOOL#0;
//	AOB_KR23_OFF := BOOL#0;
//	AOB_KR25_OFF := BOOL#0;
END_IF;

// Тело drum
CASE drumAOB.step OF
0:	IF drumAOB.step_prev <> INT#0 THEN
		drumAOB.TMR2 := LREAL#120;
		drumAOB.step_prev := drumAOB.step;
	END_IF;
	AOB_to_DO := BOOL#1; (*АОбс в СтУ-1, ГПА11-14*)
	(*КХР открыть*)
//	AOB_KR36_ON := BOOL#1;
//	AOB_KR36a_ON := BOOL#1;
//	AOB_KR23_OFF := BOOL#1;
//	AOB_KR25_OFF := BOOL#1;
	drumAOB.TMR2 := drumAOB.TMR2 - cycleTime_MAIN;
	(* IF (CTU1_SHKU17_A1.KR7_OFF AND NOT CTU1_SHKU17_A1.KR7_ON) AND (CTU1_SHKU17_A1.KR7a_OFF AND NOT CTU1_SHKU17_A1.KR7a_ON) AND (CTU1_SHKU17_A1.KR8_OFF AND NOT CTU1_SHKU17_A1.KR8_ON) AND
	   (SHKU17_DI_HMI_A1.KR36_ON AND NOT SHKU17_DI_HMI_A1.KR36_OFF) AND (SHKU17_DI_HMI_A1.KR36a_ON AND NOT SHKU17_DI_HMI_A1.KR36a_OFF) AND
	   (SHKU17_DI_HMI_A1.KR36p_ON AND NOT SHKU17_DI_HMI_A1.KR36p_OFF) AND
	   (SHKU17_DI_HMI_A1.KR23_OFF AND NOT SHKU17_DI_HMI_A1.KR23_ON) AND (SHKU17_DI_HMI_A1.KR25_OFF AND NOT SHKU17_DI_HMI_A1.KR25_ON) THEN
		drumAOB.step := INT#1;
	END_IF; *)
	IF drumAOB.TMR2 <= LREAL#0 THEN
		drumAOB.step := INT#2;
	END_IF;

1:	drumAOB.done_ok := BOOL#1;
	drumAOB.step_prev := drumAOB.step;
	drumAOB.step := INT#3;

2:	drumAOB.done_flt := BOOL#1;
	drumAOB.step_prev := drumAOB.step;
	drumAOB.step := INT#3;

3:	IF drumAOB.step_prev <> INT#3 THEN
//		Need_accept.AOB_MG_cut := BOOL#1;
		AOB_to_DO := BOOL#0;
//		AOB_KR36_ON := BOOL#0;
//		AOB_KR36a_ON := BOOL#0;
//		AOB_KR23_OFF := BOOL#0;
//		AOB_KR25_OFF := BOOL#0;
		drumAOB.step_prev := drumAOB.step;
	END_IF;
	IF Deblock AND NOT drumAOB.start THEN
		drumAOB.step := INT#-1;
		drumAOB.step_prev := INT#-1;
		drumAOB.run := BOOL#0;
		drumAOB.done_ok := BOOL#0;
		drumAOB.done_flt := BOOL#0;
	END_IF;		
END_CASE;

IF Global_BTN.AOB_MG_cut_accept OR Global_BTN.AOB_MG_cut_not_accept THEN
//	Need_accept.AOB_MG_cut := BOOL#0;
END_IF;

// Выходы
//SHKU17_DO_HMI_A1.AObs_main_GPA11	:= AOB_to_DO;
//SHKU17_DO_HMI_A1.AObs_backup_GPA11	:= AOB_to_DO;
//SHKU17_DO_HMI_A1.AObs_main_GPA12	:= AOB_to_DO;
//SHKU17_DO_HMI_A1.AObs_backup_GPA12	:= AOB_to_DO;
//SHKU17_DO_HMI_A1.AObs_main_GPA13	:= AOB_to_DO;
//SHKU17_DO_HMI_A1.AObs_backup_GPA13	:= AOB_to_DO;]]></ST>
                </FunctionBlock>
                <FunctionBlock UUID="4D53F5474B560A5B5A0762874A2A180C" Name="drum_AOS">
                    <Variables>
                        <Variable UUID="A65C5BE040510776B17054B512B5AE0E" Name="USL_1" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
                        <Variable UUID="3CDB11DE4DFB1AE5D6178EAE08D6A26F" Name="USL_2" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
                    </Variables>
                    <ST><![CDATA[drumAOS.start := (APS_TASK = INT#6) AND MD.AOS;

// Пуск drum
IF drumAOS.start AND NOT drumAOS.run THEN
	drumAOS.run := BOOL#1;
	drumAOS.step := INT#0;
END_IF;

// Останов drum извне
IF MD.EAO_KS OR MD.EAO_KC2 OR FirstScan_MAIN THEN
	drumAOS.run		:= BOOL#0;
	drumAOS.step_prev := INT#-1;
	drumAOS.step	:= INT#-1;
	AOS_to_DO		:= BOOL#0;
	AOS_KR6B_ON		:= BOOL#0;
	AOS_KR6BR_ON	:= BOOL#0;
	AOS_KR7B_OFF	:= BOOL#0;
	AOS_KR8B_OFF	:= BOOL#0;
	AOS_KR8Bbis_OFF	:= BOOL#0;
	AOS_KR17B_ON	:= BOOL#0;
	AOS_KR18B_ON	:= BOOL#0;
END_IF;

// Тело drum
CASE drumAOS.step OF
0:	IF drumAOS.step_prev <> INT#0 THEN
		drumAOS.TMR2 := LREAL#120;
		drumAOS.step_prev := drumAOS.step;
	END_IF;
	AOS_to_DO 		:= BOOL#1; (*АОсс в другие подсистемы*)
	AOS_KR6B_ON		:= BOOL#1;
	AOS_KR6BR_ON	:= BOOL#1;
	drumAOS.TMR2	:= drumAOS.TMR2 - cycleTime_MAIN;
	IF (SHKU17_A3_DI_HMI.kr6B_ON AND NOT SHKU17_A3_DI_HMI.kr6B_OFF) AND (SHKU17_A5_DI_HMI.kr6BR_ON AND NOT SHKU17_A5_DI_HMI.kr6BR_OFF) THEN
		drumAOS.step := INT#1;
	END_IF;
	IF drumAOS.TMR2 <= LREAL#0 THEN
		drumAOS.step := INT#3;
	END_IF;


1:
	AOS_KR7B_OFF	:= BOOL#1;
	AOS_KR8B_OFF	:= BOOL#1;
	AOS_KR8Bbis_OFF	:= BOOL#1;
	AOS_KR17B_ON	:= BOOL#1;
	AOS_KR18B_ON	:= BOOL#1;
	IF (SHKU17_A3_DI_HMI.kr7B_OFF AND NOT SHKU17_A3_DI_HMI.kr7B_ON) AND (SHKU17_A3_DI_HMI.kr8B_OFF AND NOT SHKU17_A3_DI_HMI.kr8B_ON) AND 
		(SHKU17_A3_DI_HMI.kr8Bbis_OFF AND NOT SHKU17_A3_DI_HMI.kr8Bbis_ON) AND (SHKU17_A3_DI_HMI.kr17B_ON AND NOT SHKU17_A3_DI_HMI.kr17B_OFF) AND 
		(SHKU17_A3_DI_HMI.kr18B_ON AND NOT SHKU17_A3_DI_HMI.kr18B_OFF) THEN
		drumAOS.step := INT#2;
	END_IF;
	IF drumAOS.TMR2 <= LREAL#0 THEN
		drumAOS.step := INT#3;
	END_IF;

2:	drumAOS.done_ok := BOOL#1;
	drumAOS.step_prev := drumAOS.step;
	drumAOS.step := INT#4;


3:	drumAOS.done_flt := BOOL#1;
	drumAOS.step_prev := drumAOS.step;
	drumAOS.step := INT#4;


4:	IF drumAOS.step_prev <> INT#3 THEN
//		Need_accept.AOS_MG_cut := BOOL#1;
		AOS_to_DO		:= BOOL#0;
		AOS_KR6B_ON		:= BOOL#0;
		AOS_KR6BR_ON	:= BOOL#0;
		AOS_KR7B_OFF	:= BOOL#0;
		AOS_KR8B_OFF	:= BOOL#0;
		AOS_KR8Bbis_OFF	:= BOOL#0;
		AOS_KR17B_ON	:= BOOL#0;
		AOS_KR18B_ON	:= BOOL#0;
		drumAOS.step_prev := drumAOS.step;
	END_IF;
	IF Deblock AND NOT drumAOS.start THEN
		drumAOS.step := INT#-1;
		drumAOS.step_prev := INT#-1;
		drumAOS.run := BOOL#0;
		drumAOS.done_ok := BOOL#0;
		drumAOS.done_flt := BOOL#0;
	END_IF;		
END_CASE;

IF Global_BTN.AOS_MG_cut_accept OR Global_BTN.AOS_MG_cut_not_accept THEN
//	Need_accept.AOS_MG_cut := BOOL#0;
END_IF;

// Выходы
SHKU17_A3_DO_HMI.GPA7_FLT	:= AOS_to_DO;
SHKU17_A3_DO_HMI.GPA8_FLT	:= AOS_to_DO;
SHKU17_A3_DO_HMI.GPA9_FLT	:= AOS_to_DO;]]></ST>
                </FunctionBlock>
